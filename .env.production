# 生产环境配置
ENV=production

# 应用配置
APP_NAME=FastAPI Demo
APP_VERSION=1.0.0
DEBUG=false

# 服务器配置
HOST=0.0.0.0
PORT=8000

# 数据库配置
DATABASE_URL=postgresql://prod_user:prod_password@prod-db:5432/fastapi_prod

# JWT配置
JWT_SECRET_KEY=${JWT_SECRET_KEY}
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=15

# API配置
API_PREFIX=/api/v1
DOCS_URL=
REDOC_URL=